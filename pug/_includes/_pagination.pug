//pagination
ul.c-pagination
    li
        a.prev(href='#')
            i.fa.fa-angle-left
    li
        a(href='#') 1
    li
        a(href='#') 2
    li
        a.active(href='#') 3
    li
        |...
    li
        a(href='#') 10
    li
        a.next(href='#')
            i.fa.fa-angle-right
